generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        String   @id @default(uuid())
  title     String
  amount    Float
  type      String   // "Income" or "Expense"
  category  String
  date      DateTime
  userId    String
  createdAt DateTime @default(now())
}